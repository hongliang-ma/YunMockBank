    <sqlMap resource="sqlmap/Lable-sqlmap-mapping.xml"/>
    <sqlMap resource="sqlmap/TemplateDetail-sqlmap-mapping.xml"/>
    <sqlMap resource="sqlmap/TemplateSystem-sqlmap-mapping.xml"/>
    <sqlMap resource="sqlmap/TemplateUser-sqlmap-mapping.xml"/>
	
	
--></sqlMapConfig>